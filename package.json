{
  "name": "food-fight",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "npm run format:check && npm run lint && npm run lint:scss && npm run build",
    "scss:clean": "rm ./combined.css wallace-report.json",
    "scss:combine": "cat .next/static/css/*.css > ./combined.css",
    "scss:analyze": "npx wallace ./combined.css",
    "scss:json": "npx wallace ./combined.css --json > wallace-report.json",
    "scss:report": "npm run build && npm run scss:combine && npm run scss:analyze && npm run scss:json",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,scss,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,scss,md}\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "start": "next start",
    "start:all": "make start-all"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@reown/appkit": "^1.7.2",
    "@reown/appkit-adapter-wagmi": "^1.7.2",
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-query-devtools": "^5.74.4",
    "@wagmi/cli": "^2.3.1",
    "ethers": "^6.13.5",
    "framer-motion": "^12.6.3",
    "jwt-decode": "^4.0.0",
    "next": "^15.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sass": "^1.86.3",
    "viem": "^2.26.3",
    "wagmi": "^2.14.16",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5"
  }
}
